{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "curlme.io",
  "colors": {
    "primary": "#F1976A",
    "light": "#ed7940",
    "dark": "#E95C16"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "quickstart",
              "concepts"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/bins",
              "features/custom-responses",
              "features/response-rules",
              "features/replay",
              "features/export",
              "features/organizations"
            ]
          },
          {
            "group": "Security",
            "pages": [
              "security/authentication",
              "security/api-keys",
              "security/password-protection"
            ]
          }
        ]
      },
      {
        "tab": "CLI",
        "groups": [
          {
            "group": "CLI Reference",
            "pages": [
              "cli/introduction",
              "cli/installation",
              "cli/authentication",
              "cli/bins",
              "cli/requests",
              "cli/advanced"
            ]
          }
        ]
      },
      {
        "tab": "API",
        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api/introduction",
              "api/authentication"
            ]
          },
          {
            "group": "Bins",
            "pages": [
              "api/bins/list",
              "api/bins/create",
              "api/bins/get",
              "api/bins/update",
              "api/bins/delete"
            ]
          },
          {
            "group": "Requests",
            "pages": [
              "api/requests/list",
              "api/requests/webhook"
            ]
          },
          {
            "group": "Organizations",
            "pages": [
              "api/organizations/list",
              "api/organizations/create",
              "api/organizations/get",
              "api/organizations/update",
              "api/organizations/delete"
            ]
          },
          {
            "group": "User",
            "pages": [
              "api/user/me",
              "api/user/keys"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://curlme.io",
          "icon": "globe"
        },
        {
          "anchor": "Dashboard",
          "href": "https://curlme.io/dashboard",
          "icon": "chart-mixed"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/curlme-io",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/light.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://curlme.io/auth/signin"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view"
    ]
  }
}